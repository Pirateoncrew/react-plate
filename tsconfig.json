  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./build/public",
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "preserveConstEnums": true
  },
  "include": ["./src/**/*"]
}
